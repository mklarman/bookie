<% @my_date %>
<% self.get_time %>

<% if @ticket.date == @my_date %>
	
	<%= form_for @ticket do |t| %>

		<%= t.text_field :started %>
		<%= t.submit %>


	<% end %>

<% else %>

	<%= form_for @ticket do |t| %>

	<%= t.text_field :name, value: @ticket.name %>
	<%= t.text_field :score %>
	<%= t.hidden_field :updated, value: true %>
	<%= t.submit %>


	<% end %>



<% end %>

