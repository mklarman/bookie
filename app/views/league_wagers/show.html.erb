		<% else %>
		
		

			<div>WHALES</div>

			<div><%= c.league_name %></div>

				<% c.league_wagers.each do |w| %>

					<div><%= "$" + w.amount.to_s + " " + w.wager_type %></div>

					<% if w.wager_type == "straight" %>

					<div><%= w.team1 %></div>

					<% end %>
					
					<% if w.wager_type == "two team parlay" %>

						<div><%= w.team1 %></div>
						<div><%= w.team2 %></div>

					<% end %>
					
					<% if w.wager_type == "three team parlay" %>

