					<% end %>
					
					<% if w.wager_type == "three team teaser" %>

						<div><%= w.team1 %></div>
						<div><%= w.team2 %></div>
						<div><%= w.team3 %></div>

					<% end %>
					
					<% if w.wager_type == "five team teaser" %>

						<div><%= w.team1 %></div>
