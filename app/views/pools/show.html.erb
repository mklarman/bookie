<% @my_date %>
<% self.get_time %>
<% @players_left = [] %>
<% @contestants = [] %>
<% @selection_holder = [] %>
<% @needs_grading = false %>
<% self.grade_check %>

<% if @needs_grading == true %>


	<% @contestants.each do |c| %>

		<% @picks = [] %>

		<% c.selections.each do |s| %>

			<% if s.pool_id.to_i == @pool.id.to_i %>

				<% @picks.push(s) %>

			<% end %>


		<% end %> 

		<%= c.sports_book_name %>
		<%= @picks.length %> 

	<% end %>



<% else %>





<% end %>