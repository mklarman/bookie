<% @client_holder = [] %>

<% @qualified_clients = [] %>


<% @clients.each do |c| %>

	<% if c.league_id == @client.league_id %>

		<% @client_holder.push(c) %>

	<% end %>


<% end %>

<% @client_holder.each do |c| %>

	<% @wager_num = 0 %>

	<% @wagers.each do |w| %>

		<% if c.id.to_i == w.league_client_id.to_i %>

			<% @wager_num = @wager_num + 1 %> 

		<% end %>

	<% end %>

	<% if @wager_num < c.wager_limit.to_i %>

		<% @qualified_clients.push(c) %>


	<% end %>



<% end %>

<%= @qualified_clients.count %>



<% render "new" %>